public class arraymax 
{
    public static void main(String args[])
    {
        int a[]={10,100,20,50,77};
        int max=0;
        for(int i=0;i<a.length;i++)
        {
            if(a[i]>max)
            max=a[i];
            System.out.println("The max element is:");
        }
        System.out.println(max);
    }
}
